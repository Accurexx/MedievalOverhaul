	  <RecipeDef>
      <defName>DankPyon_BakeBread</defName>
      <label>bake 2 breads</label>
      <description>Bake 2 breads from flour.</description>
      <jobString>Baking bread.</jobString>  
      <allowMixingIngredients>true</allowMixingIngredients>
      <ingredients>
         <li>
            <filter>
